<h2>Student list</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Class</th>
        <th>Address</th>
    </tr>
    {{#each this.model as |student|}}
    <tr>
        <td>{{student.id}}</td>
        <td>{{student.name}}</td>
        <td>{{student.sclass}}</td>
        <td>{{student.address}}</td>
    </tr>
    {{/each}}
</table>
{{outlet}}